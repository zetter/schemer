grammar Schemer
  rule expression
    first:(atom / list) (' ' rest:expression)* {
      def to_ast
        first.to_ast
      end
    }
  end

  rule list
    '(' expression ')' {
      def to_ast
        Schemer::List.new
      end
    }
  end

  rule atom
    [^\ \( \)]+ {
      def to_ast
        Schemer::Atom.new
      end
    }
  end



end